{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": [true, "never-prefix"],
        "quotemark": [true, "single", "avoid-escape"],
        "object-literal-sort-keys": false,
        "max-line-length": [true, 140],
        "member-ordering": [false],
        "variable-name": [true, "ban-keywords"]
    },
    "rulesDirectory": []
}
